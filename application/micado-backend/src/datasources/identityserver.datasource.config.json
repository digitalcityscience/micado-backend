{
  "name": "identityserver",
  "connector": "rest",
  "baseURL": "https://identity.micadoproject.eu",
  "debug": true,
  "options": {
    "strictSSL": false
  },
  "crud": false,
  "operations": [
    {
      "template": {
        "method": "GET",
        "url": "https://identity.micadoproject.eu/t/migrants.micado.eu/api/identity/consent-mgt/v1.0/consents",
        "headers": {
          "accept": "*/*",
          "content-type": "application/json",
          "Authorization": "Basic {auth}"
        },
        "query": {
          "limit": 10,
          "offset": 0,
          "spTenantDomain": "{tenant}",
          "piiPrincipalId": "{principal}",
          "state": "ACTIVE"
        },
        "options": {
          "strictSSL": false,
          "useQuerystring": true
        }
      },
      "functions": {
        "consent": [
          "tenant",
          "principal",
          "auth"
        ]
      }
    },
    {
      "template": {
        "method": "GET",
        "url": "https://identity.micadoproject.eu/t/migrants.micado.eu/api/identity/consent-mgt/v1.0/consents/receipts/{receipt}",
        "headers": {
          "accept": "*/*",
          "content-type": "application/json",
          "Authorization": "Basic {auth}"
        },
        "options": {
          "strictSSL": false,
          "useQuerystring": true
        }
      },
      "functions": {
        "receipt": [
          "receipt",
          "auth"
        ]
      }
    }
  ]
}