{
  "name": "identityserver",
  "connector": "rest",
  "baseURL": "",
  "debug": true,
  "options": {
    "strictSSL": false
  },
  "crud": false,
  "operations": [
    {
      "template": {
        "method": "GET",
        "url": "https://{baseurl}/t/{tenant}/api/identity/consent-mgt/v1.0/consents",
        "headers": {
          "accept": "*/*",
          "content-type": "application/json",
          "Authorization": "Basic {auth}"
        },
        "query": {
          "limit": 10,
          "offset": 0,
          "spTenantDomain": "{tenant}",
          "piiPrincipalId": "{principal}",
          "state": "ACTIVE"
        },
        "options": {
          "strictSSL": false,
          "useQuerystring": true
        }
      },
      "functions": {
        "consent": [
          "tenant",
          "principal",
          "auth",
          "baseurl"
        ]
      }
    },
    {
      "template": {
        "method": "GET",
        "url": "https://{baseurl}/t/{tenant}/api/identity/consent-mgt/v1.0/consents/receipts/{receipt}",
        "headers": {
          "accept": "*/*",
          "content-type": "application/json",
          "Authorization": "Basic {auth}"
        },
        "options": {
          "strictSSL": false,
          "useQuerystring": true
        }
      },
      "functions": {
        "receipt": [
          "receipt",
          "auth",
          "baseurl",
          "tenant"
        ]
      }
    },
    {
      "template": {
        "method": "GET",
        "url": "https://{baseurl}/t/{tenant}/scim2/Groups?filter=displayName+eq+{role}",
        "headers": {
          "accept": "*/*",
          "content-type": "application/json",
          "Authorization": "{authType} {auth}"
        },
        "options": {
          "strictSSL": false,
          "useQuerystring": true
        }
      },
      "functions": {
        "getGroup": [
          "role",
          "auth",
          "baseurl",
          "tenant",
          "authType"
        ]
      }
    },
    {
      "template": {
        "method": "POST",
        "url": "https://{baseurl}/t/{tenant}/scim2/Groups",
        "headers": {
          "accept": "*/*",
          "content-type": "application/json",
          "Authorization": "{authType} {auth}"
        },
        "body": {
          "displayName": "{role}"
        },
        "options": {
          "strictSSL": false,
          "useQuerystring": true
        }
      },
      "functions": {
        "addGroups": [
          "role",
          "auth",
          "baseurl",
          "tenant",
          "authType"
        ]
      }
    },
    {
      "template": {
        "method": "PATCH",
        "url": "https://{baseurl}/t/{tenant}/scim2/Groups/{groupid}",
        "headers": {
          "accept": "*/*",
          "content-type": "application/json",
          "Authorization": "{authType} {auth}"
        },
        "body": {
          "schemas": [
            "urn:ietf:params:scim:api:messages:2.0:PatchOp"
          ],
          "Operations": [
            {
              "op": "add",
              "value": {
                "members": [
                  {
                    "display": "{username}",
                    "$ref": "{location}",
                    "value": "{userid}"
                  }
                ]
              }
            }
          ]
        },
        "options": {
          "strictSSL": false,
          "useQuerystring": true
        }
      },
      "functions": {
        "addToGroups": [
          "groupid",
          "username",
          "userid",
          "location",
          "auth",
          "baseurl",
          "tenant",
          "authType"
        ]
      }
    },
    {
      "template": {
        "method": "PATCH",
        "url": "https://{baseurl}/t/{tenant}/scim2/Users/{userid}",
        "headers": {
          "accept": "*/*",
          "content-type": "application/json",
          "Authorization": "{authType} {auth}"
        },
        "body": {
          "schemas": [
            "urn:ietf:params:scim:api:messages:2.0:PatchOp"
          ],
          "Operations": [
            {
              "op": "add",
              "value": {
                "emails": [
                  {
                    "value":"{email}"
                  }
              ],
                "name":
                  {
                    "givenName": "{givenName}",
                    "familyName": "{familyName}"
                  },
                  "userName": "{username}",
                  "phoneNumbers": [
                    {
                        "type": "mobile",
                        "value": "{phoneNumber}"
                    }
                ],
                "urn:ietf:params:scim:schemas:extension:enterprise:2.0:User": {
                  "country": "{country}",
                  "gender": "{gender}",
                  "dob": "{dob}"
              }
              }
            }
          ]
        },
        "options": {
          "strictSSL": false,
          "useQuerystring": true
        }
      },
      "functions": {
        "updateUsers": [
          "userid",
          "username",
          "givenName",
          "familyName",
          "phoneNumber",
          "country",
          "gender",
          "dob",
          "email",
          "auth",
          "baseurl",
          "tenant",
          "authType"
        ]
      }
    },
    {
      "template": {
        "method": "POST",
        "url": "https://{baseurl}/t/{tenant}/scim2/Users",
        "headers": {
          "accept": "*/*",
          "content-type": "application/json",
          "Authorization": "{authType} {auth}"
        },
        "body": {
          "schemas": [],
          "name": {
            "familyName": "{surname}",
            "givenName": "{name}"
          },
          "userName": "{username}",
          "password": "{password}",
          "emails": [
            {
              "primary": true,
              "value": "{email}",
              "type": "work"
            }
          ]
        },
        "options": {
          "strictSSL": false,
          "useQuerystring": true
        }
      },
      "functions": {
        "createUser": [
          "username",
          "password",
          "name",
          "surname",
          "email",
          "auth",
          "baseurl",
          "tenant",
          "authType"
        ]
      }
    }
  ]
}