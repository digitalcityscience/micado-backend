{
  "name": "countly",
  "connector": "rest",
  "baseURL": "https://monitoring.micadoproject.eu",
  "debug": true,
  "options": {
    "headers": {
      "accept": "*/*",
      "content-type": "application/json",
      "Authorization": "Basic YWRtaW46bWljYWRvYWRtMjAyMA=="
    },
    "strictSSL": false
  },
  "crud": false,
  "operations": [
    {
      "template": {
        "method": "GET",
        "url": "https://monitoring.micadoproject.eu/o/analytics/dashboard",
        "query": {
          "api_key": "{api_key}",
          "app_id": "{app_id}"
        },
        "options": {
          "strictSSL": false,
          "useQuerystring": true
        }
      },
      "functions": {
        "dashboard": [
          "api_key",
          "app_id"
        ]
      }
    }
  ]
}